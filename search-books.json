---
layout: nil
---
[
  {% for post in site.books %}
    {

      "title"    : "<i class=\"fas fa-link\"></i> {{ post.title | escape }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "authors"   : "{{ post.authors }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "publisher"   : "{{ post.publisher }}",
      "content"     : "{{ post.content }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
